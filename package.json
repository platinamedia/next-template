    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.24.0",
    "eslint-config-next": "^12.3.1",
    "husky": "^8.0.0",
    "prettier": "^2.6.2",
